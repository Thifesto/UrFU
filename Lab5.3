package com.company;

public class t3 {
    int a3;
    int b3;

    t3() {
        a3 = -1;
        b3 = -1;
    }

    t3(int i) {
        a3 = b3 = i;
    }

    t3 (int i, int j) {
        a3 = i;
        b3 = j;
        }

    public void Output () {
        System.out.println("a3 = " + a3);
        System.out.println("b3 = " + b3);
    }
}


public class main {

public static void main(String[] args) {
    t3 p1 = new t3();
    p1.Output();

    t3 p2 = new t3(2);
    p2.Output();

    t3 p3 = new t3(2, 4);
    p3.Output();
    }

}
