package company5.com;

public class Main {

    public static void main(String[] args) {
        sup s1 = new sup("pyat");
        s1.get();


        sub1 subclass = new sub1("chetyre", 3);
        subclass.get();


        sub2 subclass2 = new sub2("dva",'l');
        subclass2.get();

        sup sub1sup = new sub1 ("zero", 1);
        sub1sup.get();

        sup sub2sup = new sub2 ("dva", 'z');
        sub2sup.get();

    }
}

public class sup {
        protected String s1;

        sup(String s2) {
            this.s1 = s2;
        }

        public sup() {
        }

        public void get() {
            System.out.println(" Class name: " + this.getClass().getSimpleName() + "\n" +
                    " string= " + this.s1);
        }
    }
    
    public class sub1 extends sup {
    protected int i1;

    sub1 (String s2, int i2) {
        super(s2);
        this.i1 = i2;
    }

    public void get() {
        System.out.println (" Class name: " + this.getClass().getSimpleName() + "\n" +
                " string= " + s1 + "\n" +
                " int= " + this.i1);
    }

}

public class sub2 extends sup {

    protected char ch1;

    sub2 (String s2, char ch2) {
        super(s2);
        this.ch1 = ch2;
    }

    public void get() {
        System.out.println (" Class name: " + this.getClass().getSimpleName() + "\n" +
                " string= " + s1 + "\n" +
                " char= " + this.ch1);
    }
}
