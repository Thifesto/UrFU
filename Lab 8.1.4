package company.com;

public class Main {
    public static int fact(int n){
        int result;
        if (n==1)
            return 1;
        else{
            result=fact(n-1)*n;
            return result;
        }
    }

    public static void main(String[] args) {
        System.out.println ("factorial 6= "  +fact(6));
    }


}
