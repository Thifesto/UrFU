package com.company;
import java.util.Scanner;
public class z3 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.println("Введите число");
        int a = in.nextInt();
        if (a % 4 == 0 && a >= 10)
            System.out.println("Число делится без остатка на 4 и при этом оно не меньше 10");
        else
            System.out.println("Число не делится без остатка на 4 или оно меньше 10");
    }

}
