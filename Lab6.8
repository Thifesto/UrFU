package com.company;

public class t8 {
    static double osm(int a[]) {
        double sum=0;
        for (int j = 0; j < a.length; j++)
            sum+= a[j];
        sum = sum/a.length;
        return sum;

    }



}

package com.company;

public class main {

    public static void main(String[] args) {
        t8 p1 = new t8();
int[] mas = {1,2,36,90,190};

double p2 = p1.osm (mas);

System.out.println ("Srednee znachenie= " +p2);

      }
}
